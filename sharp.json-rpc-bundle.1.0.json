{
  "manifests": {
    "sharp/json-rpc-bundle": {
      "manifest": {
        "bundles": {
          "Sharp\\Bundle\\JsonRpc\\JsonRpcBundle": ["all"]
        },
        "copy-from-recipe": {
          "config/": "%CONFIG_DIR%"
        },
        "post-install-output": [
          " * Log settings",
          "   monolog.formatter.jsonrpc_request:",
          "     class: Monolog\\Formatter\\LineFormatter",
          "     arguments:",
          "       - \"[%%datetime%%] [%%extra.command%%] [%%extra.context%%] %%channel%%.%%level_name%%: %%message%% %%context%% %%extra%%\\n\"",
          "",
          "   Sharp\\Bundle\\JsonRpc\\Logger\\JsonRpcRequestProcessor:",
          "     tags:",
          "       - { name: monolog.processor }",
          " "
        ]
      },
      "files": {
        "config/packages/sharp_json_rpc.yaml": {
          "contents": [
            "json_rpc:",
            "",
            "    # Set type of claims:",
            "    #    base: for base service without reference to the product",
            "    #    smart: for projects reference to SMART",
            "    #    citymark: for projects reference to CityMark",
            "    product_type: base",
            ""
          ],
          "executable": false
        },
        "config/routes/sharp_json_rpc.yaml": {
          "contents": [
            "json_rpc_routes:",
            "    resource: '@JsonRpcBundle/config/routes/routes.yaml'",
            ""
          ],
          "executable": false
        }
      },
      "ref": "e227ae259d33374de7a514a6500373f33b685231"
    }
  }
}
