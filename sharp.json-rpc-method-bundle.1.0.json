{
  "manifests": {
    "sharp/json-rpc-method-bundle": {
      "manifest": {
        "bundles": {
          "Sharp\\Bundle\\JsonRpcMethod\\JsonRpcMethodBundle": ["all"],
          "Yoanm\\SymfonyJsonRpcHttpServer\\JsonRpcHttpServerBundle": ["all"],
          "Yoanm\\SymfonyJsonRpcParamsValidator\\JsonRpcParamsValidatorBundle": ["all"]
        },
        "copy-from-recipe": {
          "config/": "%CONFIG_DIR%"
        }
      },
      "files": {
        "config/packages/json_rpc_method.yaml": {
          "contents": [
            "json_rpc_http_server:",
            "  endpoint: '/jsonrpc/v1/'",
