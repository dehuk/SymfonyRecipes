            "  idempotent_settings:",
            "    idempotent_methods:",
            "      - none",
            "  idempotence_key_life_span: 300",
            ""
          ],
          "executable": false
        },
        "config/routes/json_rpc_method.yaml": {
          "contents": [
            "json-rpc-endpoint:",
            "    resource: '@JsonRpcHttpServerBundle/Resources/config/routing/endpoint.xml'",
            ""
          ],
          "executable": false
        }
      },
      "ref": "732e4b5d0d26ca430c2baff55f0354b3d56ba16d"
    }
  }
